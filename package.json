    "lint-staged": "^13.0.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }
