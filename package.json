{
  "name": "@beer-code-industry/common-frontend",
  "version": "0.9.0",
  "private": false,
  "description": "All components, styles and functionality shared between frontend projects.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "repository": "git://github.com/BeerCodeIndustry/common-frontend.git",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-build": "yarn build-r && npm publish --access=public",
    "build-r": "yarn prebuild-r && rollup -c",
    "prebuild-r": "rm -rf dist",
    "lint": "tsc --noEmit && eslint --no-error-on-unmatched-pattern --ext .js,.ts,.tsx . --fix",
    "prepare-husky": "husky install",
    "pre-commit": "yarn build-r && npx --no-install lint-staged"
  },
  "dependencies": {
    "@beer-code-industry/common-icons": "^1.5.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.5",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@rollup/plugin-url": "^7.0.0",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
